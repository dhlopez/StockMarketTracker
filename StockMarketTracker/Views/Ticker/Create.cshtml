@model Ticker
@using StockMarketTracker;
@{
    ViewData["Title"] = "Add Ticker";
}
<form method="post">
    <label asp-for="Symbol" class="col-md-2 control-label"></label>
    <input asp-for="Symbol" class="form-control" />
    <label asp-for="Price" class="col-md-2 control-label"></label>
    <input asp-for="Price" class="form-control" />
    @*<label asp-for="DateLastUpdated" class="col-md-2 control-label"></label>
    <input asp-for="DateLastUpdated" class="form-control" />*@
    <button asp-action="Create" asp-controller="Ticker">Add</button>
</form>